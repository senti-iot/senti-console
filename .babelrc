{
	"presets": [
		"env",
		"stage-2",
		"react"
	],
	"plugins": [
		"react-hot-loader/babel"
	],
	"env": {
		"production": {
			"plugins": [
				[
					"transform-react-remove-prop-types",
					{
						"mode": "wrap",
						"ignoreFilenames": [
							"node_modules"
						]
					}
				]
			]
		}
	}
}